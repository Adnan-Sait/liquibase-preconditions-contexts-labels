<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="building-building_log-relation" author="adnan.sait">
        <addForeignKeyConstraint baseColumnNames="building_id"
            baseTableName="building_movement_log"
            constraintName="building_building_movement_log_building_id_fk"
            referencedColumnNames="id"
            referencedTableName="building"/>
    </changeSet>

    <changeSet id="user-building_log-relation" author="adnan.sait">
        <addForeignKeyConstraint baseColumnNames="user_id"
            baseTableName="building_movement_log"
            constraintName="user_building_movement_log_user_id_fk"
            referencedColumnNames="id"
            referencedTableName="user"/>
    </changeSet>

</databaseChangeLog>